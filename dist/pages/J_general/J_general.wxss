.wraps{display: flex;justify-content: space-between;box-sizing: border-box;padding: 30rpx 20rpx;font-size: 30rpx;color: #000;align-items: center;background: #fff;margin-bottom: 20rpx;}
.wraps image{display: inline-block;width: 38rpx;height: 38rpx;}
.wraps image{display: inline-block;width: 42rpx;height: 42rpx;}
.wraps view{display: flex;align-items: center;}
.wraps view text{margin-left: 10rpx;}
.wraps view text text{color: #fea500}
.wraps text text{color: #3cc456}
.wraps2{font-size: 28rpx;color: #333;margin: 0rpx;border-bottom: 1rpx solid #eee}
.wraps3{border-bottom: 1rpx solid #eee}
.wraps4 view{display: inline-block;width: 50%;padding: 20rpx 0rpx;text-align: center;}
.wraps4 view:nth-child(1){border-right: 1rpx solid #ddd}
.title{display: block;display: flex;justify-content: space-around;font-size: 30rpx;color: #333;background: #fff;padding: 0rpx 0rpx;border-bottom: 1rpx solid #ddd;margin-top: 10rpx;}
.title text{display: inline-block;width: 25%;text-align: center;box-sizing: border-box;padding: 25rpx 0rpx;}
.title .sty{border-bottom: 5rpx solid #1ea0f7;color: #1ea0f7}
.wraps4 view{display: inline-block;}
.shouru{display: flex;padding: 30rpx 20rpx;box-sizing: border-box;background: #fff;justify-content: space-between;font-size: 28rpx;color: #000;align-items: center;border-bottom: 1rpx solid #eee}
.shouru image{display: inline-block;width: 38rpx;height: 38rpx}
.shouru text text{color: #fea500}
.shouru view text{display: block;box-sizing: border-box;padding: 5rpx 0rpx;}
.shouru view text text{display: inline-block}
.shouru2{padding-top: 0rpx;}
.wraps4{padding: 20rpx 10rpx;}
