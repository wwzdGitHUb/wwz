/* pages/l_history/l_history.wxss */
page{background: #fff}
.seach{display: block;box-sizing: border-box;padding: 20rpx 60rpx;display: flex;justify-content: space-around;font-size: 30rpx;color: #fff;background: #1ea0f7;position: fixed;top: 0rpx;left: 0rpx;width: 100%;}
.seach input{display: inline-block;width:500rpx;height: 70rpx;color: #333;border-radius: 8rpx;background: #fff;box-sizing: border-box;padding: 2rpx 10rpx;}
.seach text{display: inline-block;width: 120rpx;line-height: 70rpx;background: #fff;color: #42a841;text-align: center;border-radius: 8rpx;}
.seach .valus{display: inline-block; line-height: 70rpx;border-radius: 8rpx; width: 150rpx; background: #fff;color: #42a841;text-align: center;overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;border-top-right-radius: 0rpx;border-bottom-right-radius: 0rpx;border-right: 1rpx solid #eee}
.wrap_data{display: flex;width: 100%;box-sizing: border-box;padding: 25rpx 20rpx;font-size: 30rpx;color: #333;padding-top: 140rpx;justify-content: space-between;background: #fff;align-items: center;}
.data_wrap view{display: inline-block;}
.data_wrap view:nth-child(2){display: flex;width: 245rpx;box-sizing: border-box;padding: 10rpx 10rpx;border:1rpx solid #ddd;border-radius: 8rpx;overflow: hidden;align-items: center;justify-content: space-between;}
.data_wrap{display: flex;align-items: center;}
.data_wrap view text{display: inline-block;}
picker{height:40rpx;width: 90%;}
.data_wrap view image{display: inline-block;width: 28rpx;height: 28rpx;}
.title{display: block;display: flex;justify-content: space-around;font-size: 30rpx;color: #333;background: #fff;padding: 0rpx 0rpx;border-bottom: 1rpx solid #ddd;margin-top: 10rpx;}
.title text{display: inline-block;width: 25%;text-align: center;box-sizing: border-box;padding: 25rpx 0rpx;}
.title .sty{border-bottom: 5rpx solid #1ea0f7;color: #1ea0f7}
.counts{font-size: 30rpx;color: #333;box-sizing: border-box;padding: 20rpx 20rpx;display: flex;justify-content: space-between;background: #fff;margin-top: 0rpx;margin-bottom: 1rpx;}

.countss{box-sizing: border-box;padding: 20rpx 20rpx;font-size: 28rpx;color: #333;border-bottom: 5rpx solid #ddd;background: #fff;}
.centers{display: flex;justify-content: space-between;padding: 15rpx 0rpx;}
.centers text{display: inline-block;width: 25%;text-align: center;}
.countss image{width: 38rpx;height: 38rpx;}
.phones{display: inline-block;width: 320rpx;margin-left: 10rpx;color: #1ea0f7}
picker view{ display: inline-block; width: 100%}
.hosj{display: flex;justify-content: space-between;padding: 24rpx 20rpx;box-sizing: border-box;background: #fff;font-size: 28rpx;color: #333;}
.valus{display: inline-block;margin-left: 10rpx;color: #f26262}
.down{display: inline-block;width: 32rpx;height: 32rpx;vertical-align: middle;}
.files{display: flex;align-items: center;}
.list{font-size: 28rpx;color: #333;display: flex;justify-content: space-between;padding: 25rpx 20rpx;background: #fff;border-bottom: 1rpx solid #ddd;align-items: center;}
.list view{display: inline-block;width:20%;text-align: center;}
.list image{display: inline-block;width: 32rpx;height: 32rpx;}
.counts text{display: inline-block;width:20%;text-align: center;}
.moneys{display: block;box-sizing: border-box;padding: 20rpx 20rpx;color: #000;font-size: 32rpx;background: #fff;border-bottom: 1rpx solid #eee}
.moneys text{color: #fea500}
.list .alent{display: flex;align-items: center;justify-content: space-around;color: #fca506}
.colors{color: #fca506}
.scro{max-height: 56.2vh;}